[user]
  name = Hector Oswaldo Caballero
  email = hoc@kaloom.com
  username = oswcab
[core]
  editor = vim
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = ~/.gitignore
  pager = delta
[color]
  ui = auto
[color "branch"]
  current = yellow
  local = green
  remote = cyan
[color "diff"]
  meta = white
  frag = magenta
  old = red
  new = green
  whitespace = red reverse
[color "status"]
  added = green
  changed = yellow
  untracked = red
[diff]
  tool = vimdiff
  colorMoved = default
[delta]
#  side-by-side = true
  line-numbers = true
[alias]
  a = add
  all = add .
  bl = blame
  br = branch
  brd = branch -D
  brm = branch -m
  ca = commit --amend
  cm = commit
  cne = commit --amend --no-edit
  co = checkout
  cof = checkout --force
  cp = cherry-pick
  cpa = cherry-pick --abort
  cpc = cherry-pick --continue
  df = diff
  ft = fetch
  h = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ad) %C(bold blue)<%an>%Creset' --abbrev-commit --date=iso8601
  hh = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ad) %C(bold blue)<%an>%Creset' --abbrev-commit --date=iso8601 --name-only
  l = log
  lod = log --graph --decorate
  lol = log --oneline
  pl = pull
  ps = push
  psf = push --force
  psu = push --set-upstream
  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbi = rebase -i
  rh = reset --hard
  rs = reset
  rt = remote
  sf = show --pretty="" --name-only
  sh = stash
  smu = submodule update
  smuf = submodule update --force
  st = status
  sw = show
[protocol]
  version = 2
